 <%= render 'shared/page_title', title: 'All Chefs' %>
  <div class="row">
    <div class="col-md-4 col-md-offset-4" >
     <%= will_paginate %>
      <% @chefs.each do |chef| %>
      <div class="col-md-4 col-md-offset-4">
     <%= link_to gravatar_for(chef, size: 200), chef_path(chef) %>
     <p><h4><%= chef.chefname %></h4></p>
    </div>
    <% end %>
  </div>
</div>
<%= will_paginate %>